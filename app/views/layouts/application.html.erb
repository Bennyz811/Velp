<!DOCTYPE html>
<html>
  <head>
    <title>Velp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script>
    window.velpLogo = "<%= image_url('velp-logo.png') %>"
    window.signupIllustration = "<%= image_url('signup_illustration.png') %>"
    window.footer = "<%= image_url('velp-footer.png')%>"
    window.splash = "<%= image_url('tacos.jpg')%>"
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
